{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"src/**.csproj"
					],
					"exclude": [
						"**/obj/**",
						"**/bin/**",
						"doc/_site/**"
					]
				}
			],
			"dest": "../doc/api"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"doc/api/**.yml",
					"doc/api/index.md"
				]
			},
			{
				"files": [
					"doc/articles/**.md",
					"doc/articles/**/toc.yml",
					"doc/toc.yml",
					"doc/*.md"
				],
				"exclude": [
					"obj/**",
					"doc/_site/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"doc/images/**"
				],
				"exclude": [
					"obj/**",
					"doc/_site/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"doc/apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"doc/_site/**"
				]
			}
		],
		"dest": "doc/_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default"
		],
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false
	}
}